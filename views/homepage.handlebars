<div class="container">
<div class="flexbox row">
{{#each games as |game|}}
  
    <div class="gameCard col s3" >
      <div class="card">
        <div class="card-image">
          <img src="{{game.photo_url}}">
        </div>
        <div class="card-action">
          <a href="/game/{{game.id}}">{{game.title}}</a>
        </div>
      </div>
    </div>

  {{/each}} 
</div>

</div>